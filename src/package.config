<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <package name="stsim" displayName="The ST-Sim state-and-transition simulation model" version="3.2.4" configVersion="2.0000" url="https://github.com/ApexRMS/stsim">
    <transformers>
      <transformer name="primary" isPrimary="True" displayName="ST-Sim">
        <runtime>
          <transformer name="external" displayName="ST-Sim External"/>
          <transformer name="runtime" displayName="ST-Sim"/>
        </runtime>
        <include>
          <transformer name="stime__stats"/>
          <transformer name="stime__option"/>
          <transformer name="stime__charts"/>
          <transformer name="stime__maps"/>
          <transformer name="stime__external"/>
          <transformer name="stime__multiprocessing"/>        
        </include>
      </transformer>
      <transformer
        name="external"
        displayName="External Transformer"
        className="SyncroSim.StochasticTime.StochasticTimeExternalTransformer"
        classAssembly="SyncroSim.StochasticTime"
        extendsTransformer="runtime">
      </transformer>
      <transformer
        name="runtime"
        displayName="ST-Sim"
        className="SyncroSim.STSim.STSimTransformer"
        classAssembly="SyncroSim.STSim"
        configurationSheet="RunControl">
        <datafeeds>
          <datafeed name="stratum" displayName="Strata" dataScope="Project" displayNameSource="Terminology" displayNameColumn="PrimaryStratumLabel">
            <datasheets>
              <datasheet name="Stratum" displayName="Strata" valueMember="StratumID" displayMember="Name" displayNameSource="Terminology" displayNameColumn="PrimaryStratumLabel">
                <columns>
                  <column name="StratumID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String" fillWeight="10"/>
                  <column name="ID" dataType="Integer" isOptional="True" fillWeight="5" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0"/>
                  <column name="Color" dataType="String" isOptional="True" isReadOnly="True" autoSizeMode="None" width="35"/>
                  <column name="Legend" displayName="Map Legend" dataType="String" isOptional="True" fillWeight="20"/>
                  <column name="Description" dataType="String" isOptional="True" fillWeight="20"/>
                </columns>
                <validations>
                  <validation validationType="UniqueOrNull" columns="ID"/>
                </validations>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="secondary-stratum" displayName="Secondary Strata" dataScope="Project" displayNameSource="Terminology" displayNameColumn="SecondaryStratumLabel">
            <datasheets>
              <datasheet name="SecondaryStratum" displayName="Secondary Strata" valueMember="SecondaryStratumID" displayMember="Name" displayNameSource="Terminology" displayNameColumn="SecondaryStratumLabel">
                <columns>
                  <column name="SecondaryStratumID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String" fillWeight="10"/>
                  <column name="ID" dataType="Integer" isOptional="True" fillWeight="5" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0"/>
                  <column name="Description" dataType="String" isOptional="True" fillWeight="20"/>
                </columns>
                <validations>
                  <validation validationType="UniqueOrNull" columns="ID"/>
                </validations>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="tertiary-stratum" displayName="Tertiary Strata" dataScope="Project" displayNameSource="Terminology" displayNameColumn="TertiaryStratumLabel">
            <datasheets>
              <datasheet name="TertiaryStratum" displayName="Tertiary Strata" valueMember="TertiaryStratumID" displayMember="Name" displayNameSource="Terminology" displayNameColumn="TertiaryStratumLabel">
                <columns>
                  <column name="TertiaryStratumID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String" fillWeight="10"/>
                  <column name="ID" dataType="Integer" isOptional="True" fillWeight="5" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0"/>
                  <column name="Description" dataType="String" isOptional="True" fillWeight="20"/>
                </columns>
                <validations>
                  <validation validationType="UniqueOrNull" columns="ID"/>
                </validations>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="state-label-x" displayName="State Label X" dataScope="Project" displayNameSource="Terminology" displayNameColumn="StateLabelX">
            <datasheets>
              <datasheet name="StateLabelX" displayName="State Label X" valueMember="StateLabelXID" displayMember="Name" displayNameSource="Terminology" displayNameColumn="StateLabelX">
                <columns>
                  <column name="StateLabelXID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                  <column name="Description" dataType="String" isOptional="True"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="state-label-y" displayName="State Label Y" dataScope="Project" displayNameSource="Terminology" displayNameColumn="StateLabelY">
            <datasheets>
              <datasheet name="StateLabelY" displayName="State Label Y" valueMember="StateLabelYID" displayMember="Name" displayNameSource="Terminology" displayNameColumn="StateLabelY">
                <columns>
                  <column name="StateLabelYID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                  <column name="Description" dataType="String" isOptional="True"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="state-class" displayName="State Class" dataScope="Project">
            <datasheets>
              <datasheet name="StateClass" displayName="State Class" valueMember="StateClassID" displayMember="Name" className="SyncroSim.STSim.StateClassDataSheet" classAssembly="SyncroSim.STSim">
                <columns>
                  <column name="StateClassID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String" fillWeight="10"/>
                  <column name="StateLabelXID" displayName="State Label X" dataType="Integer"  allowDbNull="False" validationType="Datasheet" formula1="StateLabelX" fillWeight="10" displayNameSource="Terminology" displayNameColumn="StateLabelX"/>
                  <column name="StateLabelYID" displayName="State Label Y" dataType="Integer"  allowDbNull="False" validationType="Datasheet" formula1="StateLabelY" fillWeight="10" displayNameSource="Terminology" displayNameColumn="StateLabelY"/>
                  <column name="ID" dataType="Integer" isOptional="True" fillWeight="5" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0"/>
                  <column name="Color" dataType="String" isOptional="True" isReadOnly="True" autoSizeMode="None" width="35"/>
                  <column name="Legend" displayName="Map Legend" dataType="String" isOptional="True" fillWeight="20"/>
                  <column name="Description" dataType="String" isOptional="True" fillWeight="20"/>
                  <column name="IsAutoName" dataType="Boolean" isVisible="False"/>
                </columns>
                <validations>
                  <validation validationType="UniqueOrNull" columns="ID"/>
                  <validation validationType="Unique" columns="StateLabelXID|StateLabelYID"/>
                </validations>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="transition-type" displayName="Transition Type" dataScope="Project">
            <datasheets>
              <datasheet name="TransitionType" displayName="Transition Type" valueMember="TransitionTypeID" displayMember="Name" className="SyncroSim.STSim.TransitionTypeDataSheet" classAssembly="SyncroSim.STSim">
                <columns>
                  <column name="TransitionTypeID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String" fillWeight="10"/>
                  <column name="ID" dataType="Integer" isOptional="True" fillWeight="5" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0"/>
                  <column name="Color" dataType="String" isOptional="True" isReadOnly="True" autoSizeMode="None" width="35"/>
                  <column name="Legend" displayName="Map Legend" dataType="String" isOptional="True" fillWeight="20"/>
                  <column name="Description" dataType="String" isOptional="True" fillWeight="20"/>
                </columns>
                <validations>
                  <validation validationType="UniqueOrNull" columns="ID"/>
                </validations>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="transition-group" displayName="Transition Group" dataScope="Project">
            <datasheets>
              <datasheet name="TransitionGroup" displayName="Transition Group" valueMember="TransitionGroupID" displayMember="Name" className="SyncroSim.STSim.TransitionGroupDataSheet" classAssembly="SyncroSim.STSim" filter="IsAuto IS NULL OR IsAuto=0">
                <columns>
                  <column name="TransitionGroupID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                  <column name="Description" dataType="String" isOptional="True"/>
                  <column name="IsAuto" dataType="Boolean" isVisible="False"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="transition-type-group" displayName="Transition Types by Group" dataScope="Project">
            <datasheets>
              <datasheet name="TransitionTypeGroup" displayName="Transition Types by Group" className="SyncroSim.STSim.TransitionTypeGroupDataSheet" classAssembly="SyncroSim.STSim" filter="IsAuto IS NULL OR IsAuto=0">
                <columns>
                  <column name="TransitionTypeGroupID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="TransitionTypeID" displayName="Transition Type" dataType="Integer" validationType="Datasheet" formula1="TransitionType" allowDbNull="False"/>
                  <column name="TransitionGroupID" displayName="Transition Group" dataType="Integer" validationType="Datasheet" formula1="TransitionGroup" allowDbNull="False"/>
                  <column name="IsAuto" dataType="Boolean" isVisible="False"/>
                </columns>
                <validations>
                  <validation validationType="Unique" columns="TransitionTypeID|TransitionGroupID"/>
                </validations>
              </datasheet>
            </datasheets>
          </datafeed>          
           <datafeed name="transition-simulation-group" displayName="Transition Simulation Groups" dataScope="Project">
            <datasheets>
              <datasheet name="TransitionSimulationGroup" displayName="Transition Simulation Groups">
                <columns>
                  <column name="TransitionSimulationGroupID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="TransitionGroupID" displayName="Transition Simulation Group" dataType="Integer" validationType="Datasheet" formula1="TransitionGroup" allowDbNull="False"/>
                </columns>
                <validations>
                  <validation validationType="Unique" columns="TransitionGroupID"/>
                </validations>
              </datasheet>
            </datasheets>
          </datafeed>                        
          <datafeed name="transition-multiplier-type" displayName="Transition Multiplier Type" dataScope="Project">
            <datasheets>
              <datasheet name="TransitionMultiplierType" displayName="Transition Multiplier Type" valueMember="TransitionMultiplierTypeID" displayMember="Name">
                <columns>
                  <column name="TransitionMultiplierTypeID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="age-type" displayName="Age Types" dataScope="Project">
            <datasheets>
              <datasheet name="AgeType" displayName="Age Types" isSingleRow="True" className="SyncroSim.STSim.AgeTypeDataSheet" classAssembly="SyncroSim.STSim">
                <columns>
                  <column name="AgeTypeID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Frequency" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0"/>
                  <column name="MaximumAge" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="age-group" displayName="Age Groups" dataScope="Project">
            <datasheets>
              <datasheet name="AgeGroup" displayName="Age Groups" className="SyncroSim.STSim.AgeGroupDataSheet" classAssembly="SyncroSim.STSim">
                <columns>
                  <column name="AgeGroupID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="MaximumAge" displayName="Maximum Age" dataType="Integer" validationType="WholeNumber" validationCondition="Greater" formula1="0" allowDbNull="False"/>
                  <column name="ID" dataType="Integer" isOptional="True" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0"/>
                  <column name="Color" dataType="String" isOptional="True" isReadOnly="True"/>
                </columns>
                <validations>
                  <validation validationType="UniqueOrNull" columns="ID"/>
                </validations>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="attribute-group" displayName="Attribute Group" dataScope="Project">
            <datasheets>
              <datasheet name="AttributeGroup" displayName="Attribute Group" valueMember="AttributeGroupID" displayMember="Name" className="SyncroSim.STSim.AttributeGroupDataSheet" classAssembly="SyncroSim.STSim">
                <columns>
                  <column name="AttributeGroupID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                  <column name="Description" dataType="String" isOptional="True"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="state-attribute-type" displayName="State Attribute Type" dataScope="Project">
            <datasheets>
              <datasheet name="StateAttributeType" displayName="State Attribute Type" valueMember="StateAttributeTypeID" displayMember="Name">
                <columns>
                  <column name="StateAttributeTypeID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                  <column name="AttributeGroupID" displayName="Attribute Group" dataType="Integer" validationType="Datasheet" formula1="AttributeGroup" isOptional="True"/>
                  <column name="Units" dataType="String" isOptional="True"/>
                  <column name="Description" dataType="String" isOptional="True"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="transition-attribute-type" displayName="Transition Attribute Type" dataScope="Project">
            <datasheets>
              <datasheet name="TransitionAttributeType" displayName="Transition Attribute Type" valueMember="TransitionAttributeTypeID" displayMember="Name">
                <columns>
                  <column name="TransitionAttributeTypeID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                  <column name="AttributeGroupID" displayName="Attribute Group" dataType="Integer" validationType="Datasheet" formula1="AttributeGroup" isOptional="True"/>
                  <column name="Units" dataType="String" isOptional="True"/>
                  <column name="Description" dataType="String" isOptional="True"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="size-class" displayName="Size Classes" dataScope="Project">
            <datasheets>
              <datasheet name="SizeClass" displayName="Size Classes" valueMember="SizeClassID" displayMember="Name">
                <columns>
                  <column name="SizeClassID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                  <column name="MaximumSize" displayName="Maximum Size" dataType="Double" allowDbNull="False"/>
                  <column name="Description" dataType="String" isOptional="True"/>
                </columns>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="terminology" displayName="Terminology" dataScope="Project">
            <datasheets>
              <datasheet name="Terminology" displayName="Terminology" isSingleRow="True">
                <columns>
                  <column name="TerminologyID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="AmountLabel" dataType="String" displayName="Amount Label"/>
                  <column name="AmountUnits" dataType="Integer" displayName="Amount Units" validationType="List" formula1="1:Acres|2:Hectares|3:Square Kilometers|4:SquareMiles"/>
                  <column name="StateLabelX" dataType="String" displayName="State Label X"/>
                  <column name="StateLabelY" dataType="String" displayName="State Label Y"/>
                  <column name="PrimaryStratumLabel" dataType="String" displayName="Primary Stratum Label"/>
                  <column name="SecondaryStratumLabel" dataType="String" displayName="Secondary Stratum Label"/>
                  <column name="TertiaryStratumLabel" dataType="String" displayName="Tertiary Stratum Label"/>
                  <column name="TimestepUnits" dataType="String" displayName="Timestep Units"/>
                </columns>
                <defaultRecords>
                  <record columns="AmountLabel|AmountUnits|StateLabelX|StateLabelY|PrimaryStratumLabel|SecondaryStratumLabel|TertiaryStratumLabel|TimestepUnits" values="Area|1|Class|Subclass|Primary Stratum|Secondary Stratum|Tertiary Stratum|Timestep"/>
                </defaultRecords>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="patch-prioritization" displayName="Patch Prioritization" dataScope="Project">
            <datasheets>
              <datasheet name="PatchPrioritization" displayName="Patch Prioritization" valueMember="PatchPrioritizationID" displayMember="Name">
                <columns>
                  <column name="PatchPrioritizationID" dataType="Integer" isPrimary="True"/>
                  <column name="ProjectID" dataType="Integer"/>
                  <column name="Name" dataType="String"/>
                </columns>
                <defaultRecords>
                  <record columns="Name" values="Smallest"/>
                  <record columns="Name" values="Smallest (transition edges only)"/>
                  <record columns="Name" values="Largest"/>
                  <record columns="Name" values="Largest (transition edges only)"/>
                </defaultRecords>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="run-control" displayName="Run Control" dataScope="Scenario">
            <datasheets>
              <datasheet name="RunControl" displayName="Run Control" isSingleRow="True">
                <columns>
                  <column name="RunControlID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="MinimumIteration" displayName="Minimum Iteration" dataType="Integer" defaultValue="1" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="1"/>
                  <column name="MaximumIteration" displayName="Maximum Iteration" dataType="Integer" defaultValue="1" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="1" format="d"/>
                  <column name="MinimumTimestep" displayName="Minimum Timestep" dataType="Integer" defaultValue="0" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" format="d"/>
                  <column name="MaximumTimestep" displayName="Maximum Timestep" dataType="Integer" defaultValue="10" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" format="d"/>
                  <column name="IsSpatial" dataType="Boolean"/>
                </columns>
                <validations>
                  <validation validationType="LessEqual" columns="MinimumIteration|MaximumIteration"/>
                  <validation validationType="LessEqual" columns="MinimumTimestep|MaximumTimestep"/>
                </validations>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="transition" displayName="Transition Pathways" dataScope="Scenario">
            <datasheets>
              <datasheet name="DeterministicTransition" displayName="States" className="SyncroSim.STSim.DeterministicTransitionDataSheet" classAssembly="SyncroSim.STSim" noRowLimit="True">
                <columns>
                  <column name="DeterministicTransitionID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="Iteration" dataType="Integer" validationType="WholeNumber" validationCondition="Greater" formula1="0" isOptional="True" format="d"/>
                  <column name="Timestep" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" isOptional="True" displayNameSource="Terminology" displayNameColumn="TimestepUnits" format="d"/>
                  <column name="StratumIDSource" dataType="Integer" displayName="Stratum" validationType="Datasheet" formula1="Stratum" displayNameSource="Terminology" displayNameColumn="PrimaryStratumLabel" isOptional="True"/>
                  <column name="StateClassIDSource" dataType="Integer" displayName="Class" validationType="Datasheet" formula1="StateClass" allowDbNull="False"/>
                  <column name="StratumIDDest" dataType="Integer" displayName="To Stratum" validationType="Datasheet" formula1="Stratum" isOptional="True"/>
                  <column name="StateClassIDDest" dataType="Integer" displayName="To Class" validationType="Datasheet" formula1="StateClass" isOptional="True"/>
                  <column name="AgeMin" displayName="Age Min" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" isOptional="True"/>
                  <column name="AgeMax" displayName="Age Max" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" isOptional="True"/>
                  <column name="Location" dataType="String" columnAlignment="Center" allowDbNull="False"/>
                </columns>
                <validations>
                  <validation validationType="LessEqual" columns="AgeMin|AgeMax"/>
                  <validation validationType="Unique" columns="StratumIDSource|StateClassIDSource"/>
                  <validation validationType="Unique" columns="StratumIDSource|Location"/>
                </validations>
              </datasheet>
              <datasheet name="Transition" displayName="Transitions" className="SyncroSim.STSim.ProbabilisticTransitionDataSheet" classAssembly="SyncroSim.STSim" noRowLimit="True">
                <columns>
                  <column name="TransitionID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer" isCompositeIndex="True"/>
                  <column name="Iteration" dataType="Integer" validationType="WholeNumber" validationCondition="Greater" formula1="0" isOptional="True" format="d"/>
                  <column name="Timestep" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" isOptional="True" displayNameSource="Terminology" displayNameColumn="TimestepUnits" format="d"/>
                  <column name="StratumIDSource" dataType="Integer" displayName="Stratum" validationType="Datasheet" formula1="Stratum" displayNameSource="Terminology" displayNameColumn="PrimaryStratumLabel" isOptional="True"/>
                  <column name="StateClassIDSource" dataType="Integer" displayName="Class" validationType="Datasheet" formula1="StateClass" allowDbNull="False"/>
                  <column name="StratumIDDest" dataType="Integer" displayName="To Stratum" validationType="Datasheet" formula1="Stratum" isOptional="True"/>
                  <column name="StateClassIDDest" dataType="Integer" displayName="To Class" validationType="Datasheet" formula1="StateClass" isOptional="True"/>
                  <column name="SecondaryStratumID" dataType="Integer" displayName="Secondary Stratum" validationType="Datasheet" formula1="SecondaryStratum" isOptional="True" displayNameSource="Terminology" displayNameColumn="SecondaryStratumLabel"/>
                  <column name="TertiaryStratumID" dataType="Integer" displayName="Tertiary Stratum" validationType="Datasheet" formula1="TertiaryStratum" isOptional="True" displayNameSource="Terminology" displayNameColumn="TertiaryStratumLabel"/>
                  <column name="TransitionTypeID" dataType="Integer" displayName="Transition Type" validationType="Datasheet" formula1="TransitionType" allowDbNull="False"/>
                  <column name="Probability" dataType="Double" validationType="Decimal" allowDbNull="False"/>
                  <column name="Proportion" dataType="Double" validationType="Decimal" isOptional="True"/>
                  <column name="AgeMin" displayName="Age Min" dataType="Integer" isOptional="True" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0"/>
                  <column name="AgeMax" displayName="Age Max" dataType="Integer" isOptional="True" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0"/>
                  <column name="AgeRelative" displayName="Age Shift" dataType="Integer" isOptional="True" validationType="WholeNumber"/>
                  <column name="AgeReset" displayName="Age Reset" dataType="Boolean" isOptional="True" />
                  <column name="TSTMin" displayName="TST Min" dataType="Integer" isOptional="True" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0"/>
                  <column name="TSTMax" displayName="TST Max" dataType="Integer" isOptional="True" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0"/>
                  <column name="TSTRelative" displayName="TST Shift" dataType="Integer" isOptional="True" validationType="WholeNumber"/>
                </columns>
                <validations>
                  <validation validationType="LessEqual" columns="AgeMin|AgeMax"/>
                  <validation validationType="LessEqual" columns="TSTMin|TSTMax"/>
                </validations>
              </datasheet>
            </datasheets>
          </datafeed>
          <datafeed name="initial-conditions-non-spatial" displayName="Initial Conditions Non Spatial" dataScope="Scenario">
            <datasheets>
              <datasheet name="InitialConditionsNonSpatial" displayName="Initial Conditions Non Spatial" isSingleRow="True">
                <columns>
                  <column name="InitialConditionsNonSpatialID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer"/>
                  <column name="TotalAmount" displayName="Total Amount" dataType="Double" validationType="Decimal" validationCondition="GreaterEqual" formula1="0.0"/>
                  <column name="NumCells" displayName="Number of Cells" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0"/>
                  <column name="CalcFromDist" displayName="Calculate from Distribution" dataType="Boolean"/>
                </columns>
              </datasheet>
              <datasheet name="InitialConditionsNonSpatialDistribution" displayName="Initial Cond. Distribution">
                <columns>
                  <column name="InitialConditionsNonSpatialDistributionID" dataType="Integer" isPrimary="True"/>
                  <column name="ScenarioID" dataType="Integer" isCompositeIndex="True"/>
                  <column name="Iteration" dataType="Integer" validationType="WholeNumber" validationCondition="Greater" formula1="0" isOptional="True" format="d"/>
                  <column name="StratumID" dataType="Integer" displayName="Stratum" validationType="Datasheet" formula1="Stratum" allowDbNull="False" displayNameSource="Terminology" displayNameColumn="PrimaryStratumLabel"/>
                  <column name="SecondaryStratumID" dataType="Integer" displayName="Secondary Stratum" validationType="Datasheet" formula1="SecondaryStratum" isOptional="True" displayNameSource="Terminology" displayNameColumn="SecondaryStratumLabel"/>
                  <column name="TertiaryStratumID" dataType="Integer" displayName="Tertiary Stratum" validationType="Datasheet" formula1="TertiaryStratum" isOptional="True" displayNameSource="Terminology" displayNameColumn="TertiaryStratumLabel"/>
                  <column name="StateClassID" dataType="Integer" displayName="State Class" validationType="Datasheet" formula1="StateClass" allowDbNull="False"/>
                  <column name="AgeMin" displayName="Age Min" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" isOptional="True"/>
                  <column name="AgeMax" displayName="Age Max" dataType="Integer" validationType="WholeNumber" validationCondition="GreaterEqual" formula1="0" isOptional="True"/>
                  <column name="RelativeAmount" displayName="Relative Amount" dataType="Double" validationType="Decimal" validationCondition="GreaterEqual" formula1="0.0" allowDbNull="False"/>
